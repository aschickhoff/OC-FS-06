<p style="color: green"><%= notice %></p>

<h1>Activity names</h1>

<div id="activity_names">
  <% @activity_names.each do |activity_name| %>
    <%= render activity_name %>
    <p>
      <%= link_to "Show this activity name", activity_name %>
    </p>
  <% end %>
</div>

<%= link_to "New activity name", new_activity_name_path %>
